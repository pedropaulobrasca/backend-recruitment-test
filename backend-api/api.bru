meta {
  name: api
  type: graphql
  seq: 2
}

post {
  url: {{baseUrl}}/api
  body: graphql
  auth: none
}

body:graphql {
  query {
    enterprises {
      id
      name
      commercial_name
    }
    
    enterprise(id: "1") {
      id
      name
      commercial_name
      cnpj
      description
    }
  }
}
